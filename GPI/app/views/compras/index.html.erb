<div class="container" style="margin-top: 100px">
	<%= @gg.each do |compra|%>


	<ul class="shadow border border-dark list-inline bg-light">
		<li class=" list-inline-item bg-primary"> producto: <%=compra.producto%> </li>
		<li class=" shadow p-1  mb-1 list-inline-item bg-success"> provedor: <%=compra.provedor%></li>
		<li class=" shadow p-1  mb-1 list-inline-item bg-danger"> precio: <%=compra.precio%></li>
		<li class=" shadow p-1  mb-1 list-inline-item bg-light"><%= link_to 'eliminar', data: {confirm: 'desea borrar esta cuenta?'}, action: "destroy", :id => compra.id %></li><br>

	</ul>

<%end%>

</div>