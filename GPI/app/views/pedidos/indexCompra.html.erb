<div class="container" style="margin-top: 100px">
	<%= @gg.each do |pedido|%>


	<ul class="list-inline bg-light">
		<li class=" border-list-inline-item bg-primary"> Codigo: <%=pedido.codigo%> </li>
		<li class="  list-inline-item bg-light"> Trabajo: <%=pedido.trabajo%></li>
		<li class="  list-inline-item bg-light"> Material: <%=pedido.material%></li>
		<li class="  list-inline-item bg-light"> Cantidad: <%=pedido.cantidad%></li>
		<li class=" list-inline-item bg-light"> Solicitante: <%=pedido.solicitante%></li>
		<li class="  list-inline-item bg-light"> Detalles: <%=pedido.detalles%></li>
		<li class="  list-inline-item bg-success"> Estado: <%=pedido.estado%></li>
		<li class="list-inline-item bg-light"> <%= link_to 'editar', action: "edit", :id => pedido.id %></li>
		<li class="  list-inline-item bg-light"><%= link_to 'eliminar', data: {confirm: 'desea borrar esta cuenta?'}, action: "destroy", :id => pedido.id %></li><br>

	</ul>

<%end%>

</div>